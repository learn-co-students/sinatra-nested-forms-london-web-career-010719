<h1>Pirate</h1>

<div class="pirate">
  <h3><%= @pirate.name %></h3><br>
  <h4><%= @pirate.weight%></h4>
  <h4><%= @pirate.height%></h4>
</div><br>

<h1>Ships</h1>
<% @ships.each do |ship| %>
  <div class="ship">
    <p><%= ship.name %></p><br>
    <p>Topic: <%= ship.type %></p><br>
    <p>Topic: <%= ship.booty %></p><br>
  </div><br>
<% end %>
